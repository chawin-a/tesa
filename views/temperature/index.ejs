<!DOCTYPE html>
<html>
  <head>
      <%- include('../../public/script/header') %>
    <title>Temperature</title>

  </head>
  <body>
    <%- include('../../public/script/navbar') %>
    <div>

     
     
    <div class ="content container">
       
      <div id="body-content">
              <div class="">
                <div class="subpage__header">
                  <h1>Temperature</h1>
                </div>
                <a class="active btn btn-success" href="/temperature/get">Update Temperature</a>
              </div>
          <!-- Content box-->
          <div class="" id="content-box">
              <table id="searchable-table" class="display">
                  <% if(temperature.length>0) { %>
                  <thead>
                      <tr>
                          <th>ID</th>
                          <th>sensID</th>
                          <th>Val</th>
                          <th>Date</th>
                      </tr>
                  </thead>
                  <tbody></tbody>
              </table>
          
         </div>
         <div id="footer" class="header hide-desktop show-mobile">
              <a href="#">Scroll to top</a>
          </div>
          <!-- Close body content-->
        </div>
          <!-- Templates -->
          <script id="datatable-template" type="">
              <% for(var i=0; i<temperature.length;i++) { %> 
              <tr class="row100 body">
                  <td><%= temperature[i].TeamID %></td>
                  <td><%= temperature[i].sensID %></td>
                  <td><%= temperature[i].val %></td>
                  <td><%= temperature[i].date.toLocaleString() %></td>
                </tr>
                
                <% } %>

                <% } else { %>
                  <div>No temperature found.</div>
                <% } %>
      </script>

     
    
  
  <script src="/datatable-template-master/js/base.js"></script>
  <script src="/datatable-template-master/js/datatable-tabletop-load.js"></script>
  
  
    
  </body>
</html>
