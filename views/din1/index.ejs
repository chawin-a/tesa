<!DOCTYPE html>
<html lang="en">
<head>
	<title>Temperature</title>
  <%- include('../../public/script/header') %>
  
 
</head>
<body>
	<%- include('../../public/script/navbar') %>

	<div class="container limiter">
		<div class="row ">
			<div class="col-sm-9 subpage__header">
              <h1>Digital Input</h1>
		  </div>
		  <div class="col-sm-3 subpage__btn">
          		<a class="active btn btn-success" href="/din1/get">Update Digital Input</a>
		  </div>
		</div>
	</div> 
 



   <div class="container-table100" style="background: transparent; text-align:center;"  >
			<div class="wrap-table100">
				
				<div class="table100 ver3 m-b-110">
					<div class="table100-head">
						<table>
							
							<thead>
                  <% if(din1.length>0) { %>
								<tr class="row100 head">
									<th class="cell100 column5">ID</th>
									<th class="cell100 column5">SensID</th>
									<th class="cell100 column5">Value</th>
									<th class="cell100 column5">Date</th>
								</tr>
							</thead>
						</table>
					</div>

					<div class="table100-body js-pscroll">
						<table>
							
							<tbody>
                  <% for(var i=0; i<din1.length;i++) { %> 
								<tr class="row100 body">
									<td class="cell100 column5"><%= din1[i].TeamID %></td>
									<td class="cell100 column5"><%= din1[i].sensID %></td>
									<td class="cell100 column5"><%= din1[i].val %></td>
									<td class="cell100 column5"><%= din1[i].date.toLocaleString() %></td>
									
								</tr>
              </tbody>
              <% } %>
              <% } else { %>
                <div>No Digital Input 1 found.</div>
              <% } %>
						</table>
								

				                
					</div>
				</div>

				
			</div>
		</div>
	</div>


  <%- include('../../public/script/tails') %>

</body>
</html>