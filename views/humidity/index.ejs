<!DOCTYPE html>
<html>
  <head>
    <title>Temperuature</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="container">
      <h3><a href="/humidity/get">Get Humidity</a></h3>
      <h1>Humidity</h1>
      <% if(humidity.length>0) { %>
      <table class="list-table">
        <thead>
          <tr>
            <th>sensID</th>
            <th>val</th>
            <th>date</th>
          </tr>
        </thead>
        <tbody>

          <% for(var i=0; i<humidity.length;i++) { %>
            <tr>
              <td><%= humidity[i].sensID %></td>
              <td><%= humidity[i].val %></td>
              <td><%= humidity[i].date %></td>
            </tr>
          <% } %>
        </tbody>
      </table> 
      <% } else { %>
        <div>No humidity found.</div>
      <% } %>
    </div>
  </body>
</html>
